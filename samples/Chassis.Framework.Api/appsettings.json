{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Clients": {
    "UriCatalog": {
      "IMockClient": {
        "Uri": "https://run.mocky.io/",
        "Oidc": "AuthServer",
        "Timeout": 10000
      }
    },
    "OidcCatalog": {
      "AuthServer": {
        "Mode": "ClientCredentials",
        "ClientConfiguration": {
          "ClientId": "",
          "ClientSecret": "",
          "Address": "https://authorization-server.com/token",
          "RevokeToken": "https://authorization-server.com/revoke"
        }
      }
    }
  }
}